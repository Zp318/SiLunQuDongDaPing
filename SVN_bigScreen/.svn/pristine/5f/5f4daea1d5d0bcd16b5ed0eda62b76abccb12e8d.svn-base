.top_head{
	width: 100%;
	height: 0.7rem;
	background: #ffffff;
	border-radius: 0.03rem;
	/*padding-left: 0.25rem;*/
}

.top_head_box{
	width: 18.70rem;
	height: 100%;
	float: left;
	margin-left: calc(50% - 9.35rem);
}

.top_head div{
	float: left;
	height: 100%;
}

.top_head_one{
	font-size: 0.38rem;
	line-height: 0.7rem;
	width:16.9rem;
}

.top_head_two {
	width: 0.5rem;
	margin-left: 0.15rem;
}

.top_head_two img {
    width: 0.4rem;
    display: inline-block;
    float: left;
    margin-top: 0.16rem;
}

.top_head_three{
	font-size: 0.28rem;
	line-height: 0.8rem;
}