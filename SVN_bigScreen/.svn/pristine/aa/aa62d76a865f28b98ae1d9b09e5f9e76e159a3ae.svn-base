.bar_model{
	width: 9.3rem;
	height: 3.26rem;
	border-radius: 0.06rem;
	background: #ffffff;
	float: left;
	margin-top: 0.07rem;
	border: solid 0.01rem #D1D9E2;
	box-sizing:border-box;
}

.bar_model:nth-child(even){
	margin-left: 0.09rem;
}

.bar_title{
	width: 100%;
	height: 0.47rem;
	border-bottom: solid 0.01rem #D1D9E2;
	box-sizing:border-box;
	padding-left: 0.4rem;
	font-size: 0.18rem;
}

.bar_title div{
	float: left;
	height: 0.47rem;
}

.bar_title_circle{
	background: #3DCCE4;
	width: 0.16rem;
	height: 0.16rem !important;
	margin-top: 0.16rem;
	border-radius: 0.08rem;
}

.bar_title_circle:nth-child(3){
	margin-left: 0.16rem;
	background: #2B9CD4;
}

.bar_title_text{
	line-height: 0.47rem;
	padding-left: 0.06rem;
}

