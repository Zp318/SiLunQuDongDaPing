.sectionTitleBox-conatner{
	height:	0.64rem;
	width: 100%;

	background: #FFFFFF;
	border-bottom: 1px solid #D1D9E2;
	border-radius: 6px 6px 0 0;
}

.rect-icon{
	width: 0.16rem;
	height: 0.16rem;
	border-radius: 50px 50px 50px 50px;
	background-color: #3DCCE4;
	float: left;
	margin-left: 0.4rem;
	margin-top: 0.24rem;
}

.section-index-title{
	float: left;
	margin-left: 0.13rem;
	opacity: 0.9;
	font-family: '微软雅黑Regular';
	font-size: 0.18rem;
	line-height: 0.64rem;
	color: #2A2A2A;
	letter-spacing: 0.3px;
}