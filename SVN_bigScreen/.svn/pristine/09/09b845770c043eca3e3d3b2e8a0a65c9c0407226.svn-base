package com.huawei.sc_mobile_fwd.comm;

/**
 * 类名: Datetime
 * 描述: 定义日期时间参数名称的JavaBean
 */
public class Datetime
{
    /**
     * 开始时间（YMD格式）参数名
     */
    private String startTimeName;
    
    /**
     * 结束时间（YMD格式）参数名
     */
    private String endTimeName;
    
    /**
     * 开始时间（UTC格式）参数名
     */
    private String startTimeUtcName;
    
    /**
     * 结束时间（UTC格式）参数名
     */
    private String endTimeUtcName;
    
    /**
     * 开始日期参数名（用于数据库表后缀）
     */
    private String startDayName;
    
    /**
     * 结束日期参数名（用于数据库表后缀）
     */
    private String endDayName;
    
    /** 
     * 描述: 构造函数
     */
    public Datetime()
    {
        super();
    }
    
    /** 
     * 描述: 构造函数
     * @param startTimeUtcName UTC开始时间名称
     * @param endTimeUtcName UTC结束时间名称
     */
    public Datetime(String startTimeUtcName, String endTimeUtcName)
    {
        super();
        this.startTimeUtcName = startTimeUtcName;
        this.endTimeUtcName = endTimeUtcName;
    }
    
    /** 
     * 描述: 构造函数
     * @param startTimeUtcName UTC开始时间名称
     * @param endTimeUtcName UTC结束时间名称
     * @param startDayName 开始日期名称
     * @param endDayName 结束日期名称
     */
    public Datetime(String startTimeUtcName, String endTimeUtcName, String startDayName, String endDayName)
    {
        super();
        this.startTimeUtcName = startTimeUtcName;
        this.endTimeUtcName = endTimeUtcName;
        this.startDayName = startDayName;
        this.endDayName = endDayName;
    }
    
    /** 
     * 描述: 构造函数
     * @param startTimeName YMD开始时间名称
     * @param endTimeName YMD结束时间名称
     * @param startTimeUtcName UTC开始时间名称
     * @param endTimeUtcName UCT结束时间名称
     * @param startDayName 开始日期名称
     * @param endDayName 结束日期名称
     */
    public Datetime(String startTimeName, String endTimeName, String startTimeUtcName, String endTimeUtcName,
        String startDayName, String endDayName)
    {
        super();
        this.startTimeName = startTimeName;
        this.endTimeName = endTimeName;
        this.startTimeUtcName = startTimeUtcName;
        this.endTimeUtcName = endTimeUtcName;
        this.startDayName = startDayName;
        this.endDayName = endDayName;
    }
    
    /**
     * 获取startTimeName
     * @return 返回 startTimeName
     */
    public String getStartTimeName()
    {
        return startTimeName;
    }
    
    /**
     * 设置startTimeName
     * @param startTimeName 对startTimeName进行赋值
     */
    public void setStartTimeName(String startTimeName)
    {
        this.startTimeName = startTimeName;
    }
    
    /**
     * 获取endTimeName
     * @return 返回 endTimeName
     */
    public String getEndTimeName()
    {
        return endTimeName;
    }
    
    /**
     * 设置endTimeName
     * @param endTimeName 对endTimeName进行赋值
     */
    public void setEndTimeName(String endTimeName)
    {
        this.endTimeName = endTimeName;
    }
    
    /**
     * 获取startDayName
     * @return 返回 startDayName
     */
    public String getStartDayName()
    {
        return startDayName;
    }
    
    /**
     * 设置startDayName
     * @param startDayName 对startDayName进行赋值
     */
    public void setStartDayName(String startDayName)
    {
        this.startDayName = startDayName;
    }
    
    /**
     * 获取endDayName
     * @return 返回 endDayName
     */
    public String getEndDayName()
    {
        return endDayName;
    }
    
    /**
     * 设置endDayName
     * @param endDayName 对endDayName进行赋值
     */
    public void setEndDayName(String endDayName)
    {
        this.endDayName = endDayName;
    }
    
    /**
     * 获取startTimeUtcName
     * @return 返回 startTimeUtcName
     */
    public String getStartTimeUtcName()
    {
        return startTimeUtcName;
    }
    
    /**
     * 设置startTimeUtcName
     * @param startTimeUtcName 对startTimeUtcName进行赋值
     */
    public void setStartTimeUtcName(String startTimeUtcName)
    {
        this.startTimeUtcName = startTimeUtcName;
    }
    
    /**
     * 获取endTimeUtcName
     * @return 返回 endTimeUtcName
     */
    public String getEndTimeUtcName()
    {
        return endTimeUtcName;
    }
    
    /**
     * 设置endTimeUtcName
     * @param endTimeUtcName 对endTimeUtcName进行赋值
     */
    public void setEndTimeUtcName(String endTimeUtcName)
    {
        this.endTimeUtcName = endTimeUtcName;
    }
    
}
